# eBay API Configuration
# Copy this file to .env and add your actual eBay App ID

# eBay Finding API App ID (required)
# Get this from: https://developer.ebay.com/my/keys
EBAY_APP_ID=your-ebay-app-id-here

# Optional: eBay Shopping API App ID (for detailed item information)
EBAY_SHOPPING_APP_ID=your-shopping-app-id-here

# Rate limiting settings
EBAY_REQUESTS_PER_SECOND=5
EBAY_MAX_RETRIES=3

# Search parameters
DEFAULT_MIN_PRICE=50.0
DEFAULT_MAX_RESULTS=100
DEFAULT_CATEGORY_ID=33654  # AC Compressors & Clutches

# Logging settings
LOG_LEVEL=INFO
LOG_FILE=ebay_extractor.log
